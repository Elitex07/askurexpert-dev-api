generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Customer {
  name      String
  email     String   @unique
  createdAt DateTime @default(now())
  clerkId   String   @id @unique
  panNumber String?  @unique
  status    String   @default("unverified")
}
